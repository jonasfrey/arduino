
// the setup function runs once when you press reset or power the board
void setup() {
  // initialize digital pin LED_BUILTIN as an output.
  pinMode(13, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {
  digitalWrite(13, 1);
  delay(222);
  digitalWrite(13,0);
  delay(222);
    digitalWrite(13, 1);
  delay(222);
  digitalWrite(13,0);
  delay(222);
  digitalWrite(13, 1);
  delay(2222);
  digitalWrite(13,0);
  delay(2222);
  digitalWrite(13, 1);
  delay(2222);
  digitalWrite(13,0);
  delay(2222);

}